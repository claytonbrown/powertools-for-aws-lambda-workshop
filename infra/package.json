{
  "name": "infra",
  "version": "2.0.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:destroy": "cdk destroy",
    "cdk:deploy": "cdk deploy --outputs-file cdk.out/params.json",
    "cdk:deploy:hotswap": "cdk deploy --hotswap --outputs-file cdk.out/params.json --require-approval never",
    "cdk:synth-dev": "cdk synth",
    "cdk:synth-prod": "NODE_ENV=production cdk synth"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.114.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.114.1-alpha.0",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.114.1-alpha.0",
    "@aws-cdk/aws-lambda-python-alpha": "^2.114.1-alpha.0",
    "@types/node": "^20.5.9",
    "aws-cdk": "^2.114.1",
    "esbuild": "^0.19.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-lambda-powertools/jmespath": "^2.0.4",
    "@aws-lambda-powertools/logger": "^2.0.4",
    "@aws-lambda-powertools/metrics": "^2.0.4",
    "@aws-lambda-powertools/tracer": "^2.0.4",
    "@aws-sdk/client-cognito-identity-provider": "^3.395.0",
    "@aws-sdk/client-ssm": "^3.395.0",
    "@middy/core": "^4.7.0",
    "aws-cdk-lib": "^2.114.1",
    "aws-sdk": "^2.1599.0",
    "cdk-aws-lambda-powertools-layer": "^3.7.0",
    "cdk-nag": "^2.27.216",
    "constructs": "^10.2.69",
    "imagemagick": "^0.1.3",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21",
    "yaml": "^2.3.1"
  }
}
